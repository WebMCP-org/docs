{
  "$schema": "https://mintlify.com/schema.json",
  "name": "WebMCP Documentation",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#1F5EFF",
    "light": "#4B7BFF",
    "dark": "#1449CC",
    "anchors": {
      "from": "#1F5EFF",
      "to": "#4B7BFF"
    },
    "background": {
      "dark": "#0A0A0A"
    }
  },
  "modeToggle": {
    "default": "light",
    "isHidden": false
  },
  "background": {
    "style": "gradient"
  },
  "topbarLinks": [
    {
      "name": "GitHub",
      "url": "https://github.com/MiguelsPizza/WebMCP"
    },
    {
      "name": "Discord",
      "url": "https://discord.gg/your-discord"
    }
  ],
  "topbarCtaButton": {
    "name": "Get Extension",
    "url": "https://chrome.google.com/webstore/detail/mcp-b"
  },
  "tabs": [
    {
      "name": "Documentation",
      "url": "/"
    },
    {
      "name": "API Reference",
      "url": "api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Live Demo",
      "icon": "play",
      "url": "https://mcp-b.ai"
    },
    {
      "name": "NPM Package",
      "icon": "npm",
      "url": "https://www.npmjs.com/package/@mcp-b/transports"
    },
    {
      "name": "Examples",
      "icon": "code",
      "url": "https://github.com/MiguelsPizza/mcp-b-examples"
    }
  ],
  "navigation": [
    {
      "group": "Getting Started",
      "pages": [
        "introduction",
        "quickstart",
        "examples"
      ]
    },
    {
      "group": "Guides",
      "pages": [
        "development",
        "advanced",
        "troubleshooting"
      ]
    },
    {
      "group": "Documentation Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    },
    {
      "group": "API Documentation",
      "pages": [
        "api-reference/introduction"
      ]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "api-reference/endpoint/get",
        "api-reference/endpoint/create",
        "api-reference/endpoint/delete"
      ]
    }
  ],
  "footerSocials": {
    "github": "https://github.com/MiguelsPizza/WebMCP",
    "discord": "https://discord.gg/your-discord"
